      FunctionName: Ground-dynamodb-stream-handler-dev
      Handler: elasticsearch.lambda_handler
      MemorySize: 128
      Role: !GetAtt
        - ElasticSearchStreamingLambdaRole
        - Arn
      Runtime: python3.6
      Timeout: 6
      Description: Stream data from DynamoDB to ElasticSearch
      Environment:
        Variables:
          STAGE: dev
          REGION: us-west-2
          APPLICATION: ground
          APPLICATION_NAME: Ground
          APPLICATION_PREFIX: GRD
          API_NAME: appsync
          ES_ENDPOINT: !GetAtt
            - ElasticSearchDomain
            - DomainEndpoint
          ES_INDEX: Ground-dev
          ES_REGION: us-west-2
          DEBUG: 1
    DependsOn:
      - DynamodbstreamLogGroup
      - ElasticSearchStreamingLambdaRole
